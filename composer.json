{
    "name": "secuconnect/secuconnect-php-sdk",
    "version": "0.3.0",
    "description": "",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "http://secuconnect.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "secuconnect KG",
            "homepage": "https://github.com/secuconnect/secuconnect-php-sdk"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "psr/cache": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "friendsofphp/php-cs-fixer": "~2.11"
    },
    "autoload": {
        "psr-4": { "Secuconnect\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Secuconnect\\Client\\" : "test/" }
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml.dist"
    },
    "suggest": {
        "ext-stomp": "Needed to support SmartDevice behaviour"
    }
}
